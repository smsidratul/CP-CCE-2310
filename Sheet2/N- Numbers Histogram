#include<bits/stdc++.h>
using namespace std;
int main()
{
    char s;
    cin>>s;
    long long n;
    cin>>n;
    for(int i=1;i<=n;i++)
    {
        long long j;
        cin>>j;
        for(int k=1; k<=j; k++)
        {
            cout<<s;
        }
        cout<<endl;
    }
}
